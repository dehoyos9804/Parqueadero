<resources>

    <!-- Base application theme. -->
    <style name="Theme.Parking" parent="Theme.Base"/>
    <style name="Theme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.NoParking" parent="Theme.BaseNAction"/>
    <style name="Theme.BaseNAction" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorDark</item>
        <item name="colorPrimaryDark">@color/colorDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="Theme.Initial" parent="Theme.BaseNActionInitial"/>
    <style name="Theme.BaseNActionInitial" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

</resources>
